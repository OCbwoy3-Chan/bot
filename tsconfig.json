{
	"compilerOptions": {
		"target": "es2015",
		"module": "commonjs",
		"rootDir": "src/",
		"declaration": true,
		"declarationMap": true,
		"sourceMap": true,
		"outDir": "dist/",
		"removeComments": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"baseUrl": "./src/",
		"paths": {
			"@l10n/*": [ "locale/commands/*" ],
			"@ocbwoy3chanai/*": [ "services/GenAI/*", "services/Bot/apis/OCbwoy3ChanAI/*" ],
			"@112/*": [ "lib/*" ],
			"@db/*": [ "services/Database/*" ]
		}
	}
}
